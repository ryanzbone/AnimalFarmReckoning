            <string>var ammoBoxSize = sprite_get_width(spr_ammo_box_p1);
var HUDoffset = ammoBoxSize;
var livesSize = sprite_get_width(spr_player1);

if (num_players &gt;= 2) {
    //Set up player 1 ammo display
    draw_sprite(spr_ammo_box_p1,0,HUDoffset,HUDoffset);
    player1Ammo = scr_check_ammo_type(obj_player1.ammoType);
    draw_sprite(player1Ammo,0,ammoBoxSize/2+HUDoffset,ammoBoxSize/2+HUDoffset);
    
    //Set up player 1 lives display
    draw_sprite(spr_p1_lives,0,ammoBoxSize+HUDoffset,0+HUDoffset);
    for(i = 1; i &lt;= obj_player1.playerLives; i++){
        draw_sprite(spr_player1,0,HUDoffset+ammoBoxSize + i*(livesSize+livesSize/2),HUDoffset+ammoBoxSize/2);
    }
    
    //Set up player 2 ammo display
    draw_sprite(spr_ammo_box_p2,0,room_width-ammoBoxSize-HUDoffset,0+HUDoffset);
    player2Ammo = scr_check_ammo_type(obj_player2.ammoType);;
    draw_sprite(player2Ammo,0,room_width-ammoBoxSize/2-HUDoffset,ammoBoxSize/2+HUDoffset);
    
    //set up player 2 lives display
    draw_sprite(spr_p2_lives,0,room_width-ammoBoxSize-sprite_get_width(spr_p2_lives)-HUDoffset,0+HUDoffset);
    for(i = 1; i &lt;= obj_player2.playerLives; i++){
        draw_sprite_ext(spr_player2,0,room_width -HUDoffset- ammoBoxSize - i*(livesSize+livesSize/2),ammoBoxSize/2+HUDoffset,-1,1,0,c_white,1);
    }
}

if (num_players &gt;= 3) {
    //Setup player 3 ammo display
    draw_sprite(spr_ammo_box_p3,0,HUDoffset,room_height-(2*HUDoffset));
    player3Ammo = scr_check_ammo_type(obj_player3.ammoType);
    draw_sprite(player3Ammo,0,ammoBoxSize/2+HUDoffset,room_height-(ammoBoxSize/2+HUDoffset));
    
    //Set up player 3 lives display
    draw_sprite(spr_p3_lives,0,ammoBoxSize+HUDoffset,room_height-(2*HUDoffset));
